
@{
    ViewBag.Title = "CustomersTable";
}

@model IEnumerable<prak3AspMVC.Models.Customers>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>
    <h3>Клиенты</h3>
    <table>
        <tr class="header">
            <td><p>Имя</p></td>
            <td><p>Телефон</p></td>
            <td></td>
        </tr>
        @foreach (prak3AspMVC.Models.Customers b in Model)
        {
            <tr>
                <td><p>@b.Name</p></td>
                <td><p>@b.phoneNumber</p></td>

            </tr>
        }
</div>

